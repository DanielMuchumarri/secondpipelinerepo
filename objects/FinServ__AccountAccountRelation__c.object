    <validationRules>
        <fullName>FinServ__AARCanOnlyBeDeactivated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISCHANGED(FinServ__Account__c),ISCHANGED(FinServ__RelatedAccount__c)),NOT(ISNEW()))</errorConditionFormula>
        <errorMessage>This record can&apos;t be edited. To update relationship information, you can deactivate this record and create a new one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinServ__AccountAndRelatedAccountCannotBeSame</fullName>
        <active>true</active>
        <errorConditionFormula>FinServ__Account__c = FinServ__RelatedAccount__c</errorConditionFormula>
        <errorMessage>Account and Related Account must be different entities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinServ__AccountCannotBeAnIndividual</fullName>
        <active>true</active>
        <description>Error message when any selected account is part of an individual in an AAR.</description>
        <errorConditionFormula>OR(ISPICKVAL(FinServ__Account__r.FinServ__IndividualType__c,&apos;Individual&apos;),ISPICKVAL( FinServ__RelatedAccount__r.FinServ__IndividualType__c, &apos;Individual&apos;))</errorConditionFormula>
        <errorMessage>Account can&apos;t be a part of an individual.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinServ__CannotCreateInactiveRecord</fullName>
        <active>true</active>
        <description>Ensures that a record cannot be created as Inactive</description>
        <errorConditionFormula>AND(ISNEW(),FinServ__Active__c == false)</errorConditionFormula>
        <errorMessage>A Account-Account Relationship cannot be created as Inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinServ__InactiveAARCannotBeEdited</fullName>
        <active>true</active>
        <errorConditionFormula>AND(PRIORVALUE(FinServ__Active__c) == false,NOT(ISNEW()))</errorConditionFormula>
        <errorMessage>Inactive record can&apos;t be edited. To update relationship information, you can create a new one.</errorMessage>
    </validationRules>
