    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Used in custom LWC to show content articles as part of collections</description>
    <enableActivities>true</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ReadWrite</externalSharingModel>
    <fields>
        <fullName>External_Id__c</fullName>
        <caseSensitive>true</caseSensitive>
        <description>Used in LWC Experience component for displaying specific articles (single component reference only)</description>
        <externalId>true</externalId>
        <label>Article External Id</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>SDO_ArticleContent__c</fullName>
        <description>Used in experience LWC to surface content specific to the article requested</description>
        <inlineHelpText>Full copy of the content displayed in SDO CMS component</inlineHelpText>
        <label>Article Content</label>
        <length>15000</length>
        <trackTrending>false</trackTrending>
        <type>Html</type>
        <visibleLines>10</visibleLines>
    </fields>
    <fields>
        <fullName>SDO_CMS_ContentCollection__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Used in LWC to connect articles to collections - not an MD or required as articles need the ability to stand alone</description>
        <label>Content Collection</label>
        <referenceTo>SDO_Experience_ContentCollection__c</referenceTo>
        <relationshipLabel>Content Article</relationshipLabel>
        <relationshipName>Content_Article</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>SDO_CMS_PreviewContent__c</fullName>
        <description>Text area leveraged in the SDO CMS component to provide a preview of content</description>
        <inlineHelpText>Preview of the content displayed in SDO CMS component</inlineHelpText>
        <label>Preview Content</label>
        <length>15000</length>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>10</visibleLines>
    </fields>
    <fields>
        <fullName>SDO_ImageURL__c</fullName>
        <description>Used in the custom experience component for image references</description>
        <label>Image URL</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <label>Content Article</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
